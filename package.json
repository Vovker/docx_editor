{
  "name": "electron-react-ts",
  "version": "1.1.0",
  "description": "New Electron app",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "set NODE_ENV=development && webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "set NODE_ENV=development && webpack-dev-server --config webpack.react.config.js --mode development",
    "build:electron": "set NODE_ENV=production && webpack --config webpack.electron.config.js --mode production",
    "build:react": "set NODE_ENV=production && webpack --config webpack.react.config.js --mode production",
    "build": "npm run build:electron && npm run build:react",
    "pack": "electron-builder --dir",
    "dist": "electron-builder dist 'Docx manager'"
  },
  "keywords": [],
  "author": "elisealcala",
  "license": "ISC",
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "productName": "Docx manager",
    "appId": "com.example.app",
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "css-loader": "^5.2.6",
    "electron": "^8.2.2",
    "electron-builder": "^22.7.0",
    "html-webpack-plugin": "^4.2.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.x",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/file-saver": "^2.0.5",
    "@types/pizzip": "^3.0.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "antd": "^4.22.8",
    "docxtemplater": "^3.31.3",
    "file-saver": "^2.0.5",
    "pizzip": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "redux": "^4.2.0",
    "sass": "^1.54.5",
    "sass-loader": "^10.1.1",
    "styled-components": "^5.3.5",
    "svg-inline-loader": "^0.8.2",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  }
}
